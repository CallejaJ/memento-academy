# Production environment variables
# Copy this file to prod.tfvars and fill in actual values
# NEVER commit prod.tfvars to version control

aws_region  = "us-east-1"
environment = "prod"

# Your domain name
domain_name = "memento-academy.com"

# Email for CloudWatch alarm notifications
alert_email = "your-email@example.com"

# GitHub repository (owner/repo format)
github_repo = "CallejaJ/memento-academy"

# Allowed CORS origins (add your production domains)
allowed_origins = [
  "http://localhost:3000",
  "https://*.vercel.app"
]
